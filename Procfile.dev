css: touch ../../assets/css/{output,override,override-fonts}.css && npx @tailwindcss/cli -i assets/css/input.css -o ../../assets/css/output.css --watch
hugo: cd ../.. && hugo server --bind=0.0.0.0 --cleanDestinationDir --logLevel debug  --disableFastRender --gc --minify --noHTTPCache --printI18nWarnings --buildDrafts
